public class EmpleadoBaseMasComision extends EmpleadoPorComision{
	private float salarioBase;
	public EmpleadoBaseMasComision(String nombre, String apellido, String nss, int ventasTotales, float tasaComision, float salarioBase){
		super.setDatos(nombre, apellido, nss);
		super.setventasTotales(ventasTotales);
		super.settasaComision(tasaComision);
		this.salarioBase=salarioBase;
	}
	public void setsalarioBase(float salarioBase){
		this.salarioBase=salarioBase;
	}
	public float getsalarioBase(){
		return salarioBase;
	}
}
